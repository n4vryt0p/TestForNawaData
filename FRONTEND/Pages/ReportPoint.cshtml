@page "{handler?}"
@model FrontEnd.Pages.ReportPointModel
@{
}
@Html.AntiForgeryToken()
<div class="row">
    <div class="col-lg-12">
        <div class="card ribbon-box">
            <div class="card-header d-flex align-items-center">
                <h5 class="card-title mb-0 flex-grow-1">Points</h5>
            </div>
            <div class="card-body">
                <div id="roleGrid"></div>
            </div>
        </div>
    </div>
</div>
@section scripts{
    <script type="text/javascript">
        var $users = @Html.Raw(Json.Serialize(Model.AllDdl?.UserList));
    </script>
    <script src="~/js/report-page.js"></script>
}
