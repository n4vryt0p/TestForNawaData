<!doctype html>
<html lang="en" data-layout="vertical" data-topbar="light" data-sidebar="dark" data-sidebar-size="lg" data-preloader="enable">
<head>
    @await Html.PartialAsync("~/Pages/Shared/_title_meta.cshtml")
    @await Html.PartialAsync("~/Pages/Shared/_head_css.cshtml")
    @await RenderSectionAsync("styles", required: false)

</head>
<body>
    <!-- Begin page -->
    <div id="layout-wrapper">
        @*  @await Html.PartialAsync("~/Pages/Shared/_menu.cshtml")*@
        @await Html.PartialAsync("~/Pages/Shared/_topbar.cshtml")
        @await Html.PartialAsync("~/Pages/Shared/_sidebar.cshtml")
        <!-- ============================================================== -->
        <!-- Start right Content here -->
        <!-- ============================================================== -->
        <div class="main-content">
            <div class="page-content">
                <div class="container-fluid">
                    @await Html.PartialAsync("~/Pages/Shared/_page_title.cshtml")
                    @RenderBody()

                </div>
            </div>
            @await Html.PartialAsync("~/Pages/Shared/_footer.cshtml")
        </div>

    </div>
    <!-- END layout-wrapper -->
    @* @await Html.PartialAsync("~/Pages/Shared/_customizer.cshtml")*@

    @await Html.PartialAsync("~/Pages/Shared/_vendor_scripts.cshtml")

    <!-- profile-setting init js -->
    @*<script src="~/assets/js/pages/profile-setting.init.js"></script>*@

    <!-- App js -->
    @*<script src="~/assets/js/app.js"></script>*@
    @await RenderSectionAsync("scripts", required: false)
</body>
</html>